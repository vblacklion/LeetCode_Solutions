    "ipt_list = [7, 4, 1, 5, 0, 3, 8]\n",
    "result = []\n",
    "idx = 0\n",
    "for idx, num in enumerate(ipt_list):\n",
    "    counter = 0\n",
    "    for i in ipt_list[idx:]:\n",
    "        if num > i:\n",
    "            counter += 1\n",
    "    \n",
    "    result.append(counter)\n",
    "print(result)"
